tname.value = ''\n    }\n\n    return (\n        <form className=\"create-list\" onSubmit={handleSubmitClick}>\n            <fieldset>\n                <label htmlFor=\"name\">Add List: </label>\n                <input id=\"listname\" placeholder=\"List title\" type=\"text\" required />\n                <label htmlFor=\"category\">Category: </label>\n                <select id=\"category\">\n                    {options}\n                </select>\n                <button type=\"submit\" className=\"add\" />\n            </fieldset>\n        </form>\n    )\n}","/home/mandyalber/dev/listable/src/Components/ListNav/ListLink.js",[],{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","severity":1,"message":"81","line":1,"column":1,"nodeType":"82","endLine":121,"endColumn":2},{"ruleId":"76","replacedBy":"83"},{"ruleId":"78","replacedBy":"84"},{"ruleId":"85","severity":1,"message":"86","line":15,"column":70,"nodeType":"87","endLine":15,"endColumn":86},"no-native-reassign",["88"],"no-negated-in-lhs",["89"],"import/no-anonymous-default-export","Assign object to a variable before exporting as module default","ExportDefaultDeclaration",["88"],["89"],"react/jsx-no-duplicate-props","No duplicate props allowed","JSXAttribute","no-global-assign","no-unsafe-negation"]